<Window x:Class="FrontTerreno.AgregarPredio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontTerreno"
        mc:Ignorable="d"
        Title="Agregar Predio" Height="249" Width="367"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid>
        <Label Content="Nombre:" HorizontalAlignment="Left" Margin="31,60,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Ubicación:" HorizontalAlignment="Left" Margin="31,102,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Predio" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <TextBox x:Name="Tb_nombre" HorizontalAlignment="Left" Margin="152,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178" FontSize="20"/>
        <TextBox x:Name="Tb_ubicacion" HorizontalAlignment="Left" Margin="152,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178" FontSize="20"/>
        <Button Content="Guardar" HorizontalAlignment="Center" Margin="0,159,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Width="101" Height="35" Click="Btn_guardar"/>

    </Grid>
</Window>
